{
    "name": "webbuilders-group/silverstripe-collapsiblewidgets",
    "description": "Extends the widget area editor and makes used widgets collapsible. Works well with the advanced widget editor module.",
    "type": "silverstripe-vendormodule",
    "keywords": ["silverstripe", "widgets"],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Ed Chipman",
            "homepage": "http://webbuildersgroup.com",
            "role": "Developer"
        }
    ],
    "require":
    {
        "silverstripe/recipe-core": "~4.6",
        "silverstripe/widgets": "~2.0"
    },
    "support": {
        "issues": "https://github.com/webbuilders-group/silverstripe-collapsiblewidgets/issues"
    },
    "autoload": {
		"psr-4": {
			"WebbuildersGroup\\CollapsibleWidgets\\": "src/"
		}
    },
    "extra": {
        "expose": [
            "css",
            "images",
            "javascript"
        ],
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    }
}
